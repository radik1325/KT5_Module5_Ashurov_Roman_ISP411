<Window x:Class="Is_Prokat_Sport_Invent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Is_Prokat_Sport_Invent"
        mc:Ignorable="d"
        Title="{Binding ElementName=NewFrame ,Path=Content.Title}" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="70"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" Background="#FF76E383">
            <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center" Text="Прокат спортивного инвентаря" FontSize="25"></TextBlock>
            <Image Source="/Resoursces/logo.png" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="60,0,0,0"/>

        </Grid>
        <Frame x:Name="NewFrame" Grid.Row="1" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
